public abstract class Customer{// abstract class

protected String customerName;
protected String customerPassword;
protected double customerprice;//

public Customer(String n,String p,double pr){//
customerName=n;
customerPassword=p;
customerprice=pr;
}

public String toString(){
return "customerName:"+customerName+"customerPassword:"+customerPassword+"customerprice:"+customerprice;

}

public abstract double calculateDiscount(double price);// abstract method 

}
