public abstract class Customer{// abstract class by method calculateDiscount

protected String customerName;
protected String customerId;

public Customer(String n,String id){//constructor to initialize the attributes with the received parameters
customerName=n;
customerId=id;
}

public abstract double calculateDiscount(double price);// abstract method it returns the calculated price after discount 

public String toString(){
return "Customer Name:"+customerName+"\nCustomer Id:"+customerId; }


}
