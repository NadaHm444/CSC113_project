public abstract class Customer{// abstract class

protected String customerName;
protected String customerId;
protected double customerPrice;//the jewelry price

public Customer(String n,String id,double pr){//
customerName=n;
customerId=id;
customerPrice=pr;
}

public abstract double calculateDiscount();// abstract method 

public String toString(){
return "Customer Name:"+customerName+"\nCustomer Id:"+customerId+"\nNon-discount price:"+customerPrice;}


}
